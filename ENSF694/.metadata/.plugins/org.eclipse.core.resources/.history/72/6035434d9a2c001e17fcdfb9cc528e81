package myLibrary.Sort;

public class BubbleSort {
	
	
	public void sort(int[] array) {
		
		int temp;
		for (int i = 0; i< array.length; i++) {
			for(int j = 0; j < array.length - i; j++) {
				if(array[j] > array[j+1]) {
					temp = array[i];
					array[i] = array[j];
					array[j]= temp;
				}
			}
		}
	}
}
